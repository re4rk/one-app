<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.OneApp"
        tools:targetApi="31">
        <activity
            android:name=".ui.diPractice.DiPracticeActivity"
            android:exported="false"
            android:label="@string/title_activity_di_practice"
            android:theme="@style/Theme.OneApp" />
        <activity
            android:name=".ui.lifecycleTracker.LifecycleTackerActivity"
            android:exported="true"
            android:label="@string/title_activity_lifecycle_tacker"
            android:theme="@style/Theme.OneApp" />
        <activity
            android:name=".ui.memo.MemoActivity"
            android:exported="false"
            android:label="@string/title_activity_memo"
            android:theme="@style/Theme.OneApp" />
        <activity
            android:name=".ui.home.HomeActivity"
            android:exported="true"
            android:label="@string/title_activity_home"
            android:theme="@style/Theme.OneApp">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ui.chatRoomList.ChatRoomListActivity"
            android:exported="true"
            android:label="@string/title_activity_home"
            android:theme="@style/Theme.OneApp" />
    </application>

</manifest>
